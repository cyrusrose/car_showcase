        "clsx": "^2.0.0",
        "cmdk": "^0.2.0",
        "graphql": "^16.8.1",
        "lucide-react": "^0.288.0",
        "next": "^13.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
